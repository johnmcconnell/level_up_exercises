%div#wirebox{class: "row device"}
  .col-sm-10{class: "col-sm-offset-1 wires"}
    - wirebox.wires.each do |wire|
      - if wire.snipped?
        %div{class: "wire wire-snipped"}
      - else
        - if wire.safe?
          %div{class: "wire wire-#{wire.color} safe-wire", data:{value: wire.color}}
        -else
          %div{class: "wire wire-#{wire.color} live-wire", data:{value: wire.color}}
.row
  = haml :timer, :locals => { :timer => timer }
